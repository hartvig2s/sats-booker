# SATS Login Credentials
SATS_EMAIL=your-email@sats.no
SATS_PASSWORD=your-password

# Booking Configuration
PREFERRED_CLASSES=Yoga,Spinning,CrossFit
PREFERRED_TIMES=18:00,19:00,20:00
PREFERRED_LOCATIONS=Oslo City,Colosseum

# Schedule (cron format - default: daily at 19:05, 5 minutes after classes are released)
BOOKING_SCHEDULE=5 19 * * *

# Environment (development = show browser, production = headless)
NODE_ENV=development

# Notification settings
ENABLE_NOTIFICATIONS=true